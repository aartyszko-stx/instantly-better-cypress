{
  "compilerOptions": {
    "target": "ES2015",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "@testing-library/cypress", "@cypress/grep"],
    "module": "es2015",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "utils/*": ["cypress/utils/*"],
      "pages/*": ["cypress/pages/*"],
      "support/*": ["cypress/support/*"]
    }
  },
  "include": [
    "**/*.ts",
    "cypress/reports/html/assets/app.js",
    "utils/generateUser.ts"
  ]
}
